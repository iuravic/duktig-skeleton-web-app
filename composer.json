{
    "name": "iuravic/duktig-skeleton-web-app",
    "type": "library",
    "description": "Skeleton web application with Duktig MVC micro web framework for PHP 7.1",
    "keywords": [
        "Duktig",
        "PHP",
        "MVC",
        "web",
        "framework",
        "micro framework",
        "skeleton"
    ],
    "homepage": "https://github.com/iuravic/duktig-skeleton-web-app",
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Ivan UraviÄ‡",
            "email": "iuravic@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "iuravic/duktig-core": "dev-master",
        "http-interop/http-factory": "dev-master",
        "iuravic/duktig-http-factory-guzzle-adapter": "dev-master",
        "iuravic/duktig-symfony-router-adapter": "dev-master",
        "iuravic/duktig-middleman-adapter": "dev-master",
        "iuravic/duktig-twig-adapter": "dev-master",
        "monolog/monolog": "^1.22",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0",
        "http-interop/http-middleware": "^0.4.1",
        "iuravic/duktig-symfony-event-dispatcher-adapter": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DuktigSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DuktigSkeleton\\": "tests/functional/",
            "Duktig\\Test\\":  "vendor/iuravic/duktig-core/tests/"
        }
    }
}
